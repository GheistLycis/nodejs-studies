<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("partials/head.ejs") %> 
</head>
<body>
    <section class="container">
        <nav class="navbar">
            <%- include("partials/navbar.ejs") %>
        </nav>
        <div class="content">
            <h1>POSTS:</h1>
            <% posts.forEach(item=>{ %>
                <div class="post">
                    <% if (item.stars){ %>
                        <% for(let i = 0; i < item.stars; i++){ %>
                            <img src="images/star.pgn"> <!-- CAN'T LOAD IMG-->
                        <% } %> 
                    <% } %>
                    <h3><%= item.title %></h3>
                    <p><%= item.text %></p>
                    <br>
                </div>                
            <% }) %>
        </div>
    </section>
    <footer class="footer">
        <%- include("partials/footer.ejs") %> 
    </footer>
</body>
</html>