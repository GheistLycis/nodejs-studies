<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("partials/head.ejs") %> 
</head>
<body>
    <section class="container">
        <nav class="navbar">
            <%- include("partials/navbar.ejs") %>
        </nav>
        <h1>POSTS:</h1>
        <div class="content">
            <% posts.forEach((item)=>{ %>
                <div class="post">
                    <h3><%= item.title %> </h3>
                    <p><%= item.text %> </p>
                </div>                
            <% }) %>
        </div>
    </section>
    <footer class="footer">
        <%- include("partials/footer.ejs") %> 
    </footer>
</body>
</html>